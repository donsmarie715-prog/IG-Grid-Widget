{
  "version": 2,
  "builds": [
    {
      "src": "api/feed.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/feed",
      "dest": "/api/feed.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}