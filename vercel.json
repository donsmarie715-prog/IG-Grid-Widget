{
  "version": 2,
  "builds": [
    { "src": "api/feed.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/api/feed$", "dest": "/api/feed.js" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^(?!/api/).*", "dest": "/index.html" }
  ]
}
